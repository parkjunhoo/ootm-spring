package kr.team3.ootm.dao.inquiry_product_post;

import java.sql.Timestamp;

public class InquiryProductPostDTO {
	private int inquiry_product_post_id;
	private int product_id;
	private String member_id;
	private String inquiry_product_post_title;
	private String inquiry_product_post_content;
	private Timestamp inquiry_product_post_regdate;

	public InquiryProductPostDTO() {

	}

	// insert용
	public InquiryProductPostDTO(int product_id,String member_id ,String inquiry_product_post_title,String inquiry_product_post_content) {
		this.product_id = product_id;
		this.member_id = member_id;
		this.inquiry_product_post_title = inquiry_product_post_title;
		this.inquiry_product_post_content = inquiry_product_post_content;
	}

	// select read 용
	public InquiryProductPostDTO(int inquiry_product_post_id, int product_id, String member_id, String inquiry_product_post_title,
			String inquiry_product_post_content, Timestamp inquiry_product_post_regdate) {
		this.inquiry_product_post_id = inquiry_product_post_id;
		this.product_id = product_id;
		this.member_id = member_id;
		this.inquiry_product_post_title = inquiry_product_post_title;
		this.inquiry_product_post_content = inquiry_product_post_content;
		this.inquiry_product_post_regdate = inquiry_product_post_regdate;
	}

	public int getInquiry_product_post_id() {
		return inquiry_product_post_id;
	}

	public void setInquiry_product_post_id(int inquiry_product_post_id) {
		this.inquiry_product_post_id = inquiry_product_post_id;
	}

	public int getProduct_id() {
		return product_id;
	}

	public void setProduct_id(int product_id) {
		this.product_id = product_id;
	}

	public String getMember_id() {
		return member_id;
	}

	public void setMember_id(String member_id) {
		this.member_id = member_id;
	}

	public String getInquiry_product_post_title() {
		return inquiry_product_post_title;
	}

	public void setInquiry_product_post_title(String inquiry_product_post_title) {
		this.inquiry_product_post_title = inquiry_product_post_title;
	}

	public String getInquiry_product_post_content() {
		return inquiry_product_post_content;
	}

	public void setInquiry_product_post_content(String inquiry_product_post_content) {
		this.inquiry_product_post_content = inquiry_product_post_content;
	}

	public Timestamp getInquiry_product_post_regdate() {
		return inquiry_product_post_regdate;
	}

	public void setInquiry_product_post_regdate(Timestamp inquiry_product_post_regdate) {
		this.inquiry_product_post_regdate = inquiry_product_post_regdate;
	}
	
	
	

}
