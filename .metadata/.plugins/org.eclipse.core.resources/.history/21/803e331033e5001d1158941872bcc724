		//product id == product_id ?= quantity++
		return 1;
	}

	@Override
	public int delete(int card_id) {
		return 1;
	}

	@Override
	public ArrayList<CartDTO> selectMyCart(String id) {
		ArrayList<CartDTO> result = new ArrayList<CartDTO>();
		for(int i =0; i<5; i++) {
			result.add(new CartDTO(i, i, 0, new Date(System.currentTimeMillis() ));
		}
		return null;
	}
	
}
