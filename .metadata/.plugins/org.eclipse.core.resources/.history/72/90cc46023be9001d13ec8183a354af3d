package util.define;

enum ProductCategory{
	
	RED(3), ORANGE(10), YELLOW(21), GREEN(5), BLUE(1), INDIGO(-1), VIOLET(-11);

    private final int value;

    ProductCategory(int i) {
    	this.value = value;
	}

    ProductCategory(int value) { this.value = value; }

    public int getValue() { return value; }
}
