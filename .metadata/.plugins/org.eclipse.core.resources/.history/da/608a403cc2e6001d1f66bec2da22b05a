package kr.team3.ootm.dao.headerMenu;

import java.util.List;

import kr.team3.ootm.dao.headerSubMenu.HeaderSubMenuDTO;

public class HeaderMenuDTO {
	private int id;
	private String name;
	private int idx;
	private List<HeaderSubMenuDTO> subMenu;
	
	public HeaderMenuDTO() {
	}
	
	public HeaderMenuDTO(int id ,String name,int idx, List<HeaderSubMenuDTO> subMenu) {
		this.id = id;
		this.name = name;
		this.idx = idx;
		this.subMenu = subMenu;
	}
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}


	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public int getIdx() {
		return idx;
	}

	public void setIdx(int idx) {
		this.idx = idx;
	}

	public List<HeaderSubMenuDTO> getSubMenu() {
		return subMenu;
	}

	public void setSubMenu(List<HeaderSubMenuDTO> subMenu) {
		this.subMenu = subMenu;
	}
	
	
}